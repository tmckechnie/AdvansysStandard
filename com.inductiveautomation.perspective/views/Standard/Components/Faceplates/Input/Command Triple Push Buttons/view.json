{
  "custom": {
    "FullTagPath1": "",
    "FullTagPath2": "",
    "FullTagPath3": ""
  },
  "params": {
    "Label": "Command",
    "OperMode": 1,
    "Tag1": {
      "Enabled": true,
      "Label": "Forward",
      "Name": "Cmd/ManualStartForward"
    },
    "Tag2": {
      "Enabled": true,
      "Label": "Reverse",
      "Name": "Cmd/ManualStartReverse"
    },
    "Tag3": {
      "Enabled": true,
      "Label": "Stop",
      "Name": "Cmd/ManualStop"
    },
    "TagPath": ""
  },
  "propConfig": {
    "custom.FullTagPath1": {
      "binding": {
        "config": {
          "expression": "{view.params.TagPath} + \"/\" +{view.params.Tag1.Name}"
        },
        "transforms": [
          {
            "code": "\tif quality.isGood() and self.params.TagPath!\u003d \"\":\n\t\treturn value\n\treturn \"\"",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
    "custom.FullTagPath2": {
      "binding": {
        "config": {
          "expression": "{view.params.TagPath} + \"/\" + {view.params.Tag2.Name}"
        },
