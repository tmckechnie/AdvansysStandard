        "type": "expr"
      },
      "persistent": true
    },
    "params.Label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.OperMode": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Tag1": {
      "paramDirection": "input",
