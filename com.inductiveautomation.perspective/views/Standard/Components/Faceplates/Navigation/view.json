            },
            "persistent": true
          }
        },
        "props": {
          "image": {
            "icon": {
              "path": "material/home"
            }
          },
          "style": {
            "classes": "Standard/Popups/Faceplates/Button/Selected",
            "margin": 2
          },
          "text": ""
        },
