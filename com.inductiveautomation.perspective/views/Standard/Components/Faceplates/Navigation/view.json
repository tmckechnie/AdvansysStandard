{
  "custom": {},
  "params": {
    "CurrentView": "Home",
    "InterlockEnabled": false,
    "TagPath": ""
  },
  "propConfig": {
    "params.CurrentView": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.InterlockEnabled": {
      "binding": {
        "config": {
          "path": "/root/Interlock.props.enabled"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
