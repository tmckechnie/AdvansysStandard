      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 66
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Msg"
              },
              "type": "property"
            }
          }
