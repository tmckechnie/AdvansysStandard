{
  "custom": {
    "AlarmSummary": null,
    "Description": null,
    "InstanceName": null,
    "Output": null,
    "OutputDisplayFormat": null,
    "OutputEngUnits": null,
    "ProcessVariable": null,
    "ProcessVariableEngUnits": null,
    "ProcessVariableFormatted": "#,##0.## t/hr",
    "Setpoint": null,
    "alarmStyle": null,
    "outputFormatted": "MV:  ",
    "processVariableFormatted": "PV:  ",
    "setpointFormatted": "SP:  ",
    "styling": "Standard/Body/Symbols/ValueDisplay"
  },
  "params": {
    "TagPath": "",
    "show": {
      "Output": true,
      "ProcessVariable": true,
      "Setpoint": true
    }
  },
  "propConfig": {
    "custom.AlarmSummary": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Alarms/Summary/Information"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Description": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Metadata/Description"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.InstanceName": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Metadata/InstanceName"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Output": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Out/Output"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.OutputDisplayFormat": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Out/Output.FormatString"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.OutputEngUnits": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Metadata/OutputEngUnits"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.ProcessVariable": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/In/ProcessVariable"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.ProcessVariableDisplayFormat": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/In/ProcessVariable.FormatString"
        },
        "type": "tag"
      }
    },
    "custom.ProcessVariableEngUnits": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Metadata/ProcessVariableEngUnits"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Setpoint": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "TagPath": "{view.params.TagPath}"
          },
          "tagPath": "{TagPath}/Sts/Setpoint"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.alarmStyle": {
      "binding": {
        "config": {
          "path": "view.custom.AlarmSummary"
        },
        "transforms": [
          {
            "code": "\treturn Standard.Modules.Alarms.GetAlarmStyle(AlarmSummary\u003dvalue)\n",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.outputFormatted": {
      "binding": {
        "config": {
          "expression": "\"MV: \" + numberFormat({view.custom.Output},{view.custom.OutputDisplayFormat}) + \" \" + {view.custom.OutputEngUnits}"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.processVariableFormatted": {
      "binding": {
        "config": {
          "expression": "\"PV: \" + numberFormat({view.custom.ProcessVariable},{view.custom.ProcessVariableDisplayFormat}) + \" \" + {view.custom.ProcessVariableEngUnits}"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.setpointFormatted": {
      "binding": {
        "config": {
          "expression": "\"SP: \" + numberFormat({view.custom.Setpoint},{view.custom.ProcessVariableDisplayFormat}) + \" \" + {view.custom.ProcessVariableEngUnits}"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.styling": {
      "binding": {
        "config": {
          "struct": {
            "alarmStyleClasses": "{view.custom.alarmStyle.classes}",
            "inAlarm": "{view.custom.AlarmSummary.Overall.Total}\u003e0",
            "isSelected": "tag({view.params.TagPath}+\"/Id/Selected\")",
            "isSimulation": "tag({view.params.TagPath}+\"/Sim/Simulation\")"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\t\n\tbaseStyle \u003d \"Standard/Body/Symbols\"\n\tstyleClasses \u003d baseStyle+\"/ValueDisplay\"\n\n\t#Selected\n\tif value[\"isSelected\"]:\n\t\tstyleClasses +\u003d \" \"+baseStyle+\"/Selected\"\n\n\t#Simulation\n\tif value[\"isSimulation\"]:\n\t\tstyleClasses +\u003d \" \"+baseStyle+\"/Simulation\"\n\n\t#Alarm\n\tif value[\"inAlarm\"]:\n\t\tstyleClasses +\u003d \" \"+value[\"alarmStyleClasses\"]\n\t\n\treturn styleClasses",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.show": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50,
      "width": 100
    },
    "dropConfig": {
      "udts": [
        {
          "action": "path",
          "param": "TagPath",
          "type": "Application/Control Modules/Controller"
        }
      ]
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Setpoint",
          "tooltip": {
            "style": {
              "classes": "Standard/Body/Symbols/Label"
            }
          }
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.params.show}[{this.meta.name}]"
              },
              "type": "expr"
            }
          },
          "props.style.classes": {
            "persistent": true
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.custom.setpointFormatted"
              },
              "type": "property"
            },
            "persistent": true
          }
        },
        "props": {
          "style": {
            "margin": 2,
            "textAlign": "center"
          },
          "text": "SP:  "
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "ProcessVariable",
          "tooltip": {
            "style": {
              "classes": "Standard/Body/Symbols/Label"
            }
          }
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.params.show}[{this.meta.name}]"
              },
              "type": "expr"
            }
          },
          "props.style.classes": {
            "persistent": true
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.custom.processVariableFormatted"
              },
              "type": "property"
            },
            "persistent": true
          }
        },
        "props": {
          "style": {
            "margin": 2,
            "textAlign": "center"
          },
          "text": "PV:  "
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Output",
          "tooltip": {
            "style": {
              "classes": "Standard/Body/Symbols/Label"
            }
          }
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.params.show}[{this.meta.name}]"
              },
              "type": "expr"
            }
          },
          "props.style.classes": {
            "persistent": true
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.custom.outputFormatted"
              },
              "type": "property"
            },
            "persistent": true
          }
        },
        "props": {
          "style": {
            "margin": 2,
            "textAlign": "center"
          },
          "text": "MV:  "
        },
        "type": "ia.display.label"
      }
    ],
    "events": {
      "dom": {
        "onClick": {
          "config": {
            "script": "\ttagPath \u003d self.view.params.TagPath\n\tinstanceName \u003d self.view.custom.InstanceName\n\tdescription \u003d self.view.custom.Description\n\ttitle \u003d instanceName + \" - \" + description\n\tparamsObject \u003d {\"TagPath\":tagPath}\n\tfaceplatePath \u003d \"Standard/Control Modules/Controller/Faceplates/Faceplate\"\n\tStandard.Modules.Common.OpenFaceplate(ParamsObject\u003dparamsObject,Title\u003dtitle,FaceplatePath\u003dfaceplatePath)"
          },
          "scope": "G",
          "type": "script"
        }
      }
    },
    "meta": {
      "name": "root",
      "tooltip": {
        "enabled": true
      }
    },
    "propConfig": {
      "meta.tooltip.text": {
        "binding": {
          "config": {
            "path": "view.custom.Description"
          },
          "type": "property"
        }
      },
      "props.style.classes": {
        "binding": {
          "config": {
            "path": "view.custom.styling"
          },
          "type": "property"
        },
        "persistent": true
      }
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Standard/Body/Symbols/ValueDisplay"
      }
    },
    "type": "ia.container.flex"
  }
}