      "scroll": false,
      "width": 100
    }
  },
  "propConfig": {
    "custom.Logo": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "tagPath": "{view.custom.tagPath}"
          },
          "tagPath": "{tagPath}"
        },
        "transforms": [
          {
            "expression": "\u0027data:imagef;base64,\u0027+{value}",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.logoEdit": {
      "persistent": true
    },
    "custom.tagPath": {
      "binding": {
        "config": {
          "expression": "{session.custom.SystemInfo.TagPath} + \"/Logo\""
        },
        "type": "expr"
      },
      "persistent": true
    },
    "params.LogoFit": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 60,
      "width": 138
    }
  },
  "root": {
    "children": [
