  "params": {},
  "propConfig": {
    "custom.currentModelItem": {
      "binding": {
        "config": {
          "struct": {
            "currentModelItem": "{session.custom.CurrentModelItem}",
            "pageId": "{page.props.pageId}",
            "useSessionModelItem": "{session.custom.UseSessionModelItem}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tcurrentModelItem \u003d value[\u0027currentModelItem\u0027]\n\tuseSessionModelItem \u003d value[\u0027useSessionModelItem\u0027]\n\tpageId \u003d value[\u0027pageId\u0027]\n\tcurrentModelItem \u003d Standard.Navigation.Common.GetCurrentModelItem(CurrentModelItem\u003dcurrentModelItem,UseSession\u003duseSessionModelItem,PerspectivePageID\u003dpageId)\t\n\treturn currentModelItem",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "onChange": {
        "enabled": null,
        "script": "\t#Change Script to Update History Table\n\tif currentValue is not None and previousValue is not None:\n\t\tcurrentPath \u003d currentValue.value[\"Path\"].value\n\t\tpreviousPath \u003d previousValue.value[\"Path\"].value\n\t\t#If currentModelItemTagPath is different to previous then add value to history \n\t\tif currentPath!\u003d previousPath:\t\t\n\t\t\t#Check if not in list\n\t\t\tcurrentHistory \u003d self.getChild(\"root\").getChild(\"Model Item Selection Tab Container\").getChild(\"Current Tab History\").props.data\n\t\t\t\n\t\t\tinsert \u003d True\n\t\t\tfor row in currentHistory:\n\t\t\t\tpath \u003d row[\"ModelItem\"][\"Path\"]\n\t\t\t\tif currentPath \u003d\u003d path:\n\t\t\t\t\tinsert \u003d False\n\t\t\t\t\tbreak\n\t\t\t\t\t\t\t\t\n\t\t\tif \tinsert:\t\n\t\t\t\tmodelItems \u003d self.session.custom.ModelItems\n\t\t\t\tcurrentModelItem \u003d Standard.Navigation.Common.GetModelItemByPath(ModelItems\u003dmodelItems, Path\u003dcurrentPath)[\"data\"]\n\t\t\t\trowObject \u003d self.getChild(\"root\").getChild(\"Model Item Selection Tab Container\").getRowObject(currentModelItem)\n\t\t\t\tcurrentHistory.insert(0,rowObject)\n\t\t\t\tif len(currentHistory) \u003e 10:\n\t\t\t\t\tcurrentHistory.pop(-1)\n\t\t\t\t\t\n\t\t\t#Update Styling to Highlight row that is the same as the current model item\n\t\t\tfor row in currentHistory:\n\t\t\t\trowModelItemPath \u003d row[\"ModelItem\"][\"Path\"]\n\t\t\t\tstyle \u003d {\"background-color\":\"\"}\n\t\t\t\tif rowModelItemPath \u003d\u003d currentPath:\n\t\t\t\t\tstyle \u003d {\"background-color\":\"var(--callToAction)\"}\n\t\t\t\trow[\"Name\"][\"style\"] \u003d style\t\t"
      },
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 341,
      "width": 302
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tcurrentValue \u003d self.props.selected\n\t#Used to update ModelItemTagPaths session property if UseSessionModelItemTagPath set to True\n\t#Also used to write back to UseSessionModelItemTagPath regardless\n\tif currentValue:\n\t\t#Update ModelItemTagPaths Object\n\t\tuseSessionModelItem \u003d True\n\t\tmodelItemPath \u003d self.view.custom.currentModelItem.Path\n\t\tcontentType \u003d self.view.custom.currentModelItem.ContentType\n\t\tcurrentModelItem\u003d self.session.custom.CurrentModelItem\n\t\tperspectivePageID \u003d self.page.props.pageId\n\t\tself.session.custom.CurrentModelItem \u003d Standard.Navigation.Common.UpdateCurrentModelItem(Path \u003d modelItemPath,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t CurrentModelItem \u003d currentModelItem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t UseSession \u003d useSessionModelItem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t PerspectivePageID \u003d perspectivePageID,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t ContentType \u003d contentType)\n\t\tself.session.custom.UseSessionModelItem \u003d currentValue\n\t\t\n\t\t\n\telse:\n\t\tself.session.custom.UseSessionModelItem \u003d currentValue"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
