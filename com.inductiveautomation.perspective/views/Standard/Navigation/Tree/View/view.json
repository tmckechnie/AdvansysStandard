  },
  "propConfig": {
    "custom.CurrentModelItem": {
      "binding": {
        "config": {
          "struct": {
            "currentModelItem": "{session.custom.CurrentModelItem}",
            "pageId": "{page.props.pageId}",
            "useSessionModelItem": "{session.custom.UseSessionModelItem}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tcurrentModelItem \u003d value[\u0027currentModelItem\u0027]\n\tuseSessionModelItem \u003d value[\u0027useSessionModelItem\u0027]\n\tpageId \u003d value[\u0027pageId\u0027]\n\treturn Standard.Navigation.Common.GetCurrentModelItem(CurrentModelItem\u003dcurrentModelItem,UseSession\u003duseSessionModelItem,PerspectivePageID\u003dpageId)",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.CurrentModelItemPath": {
      "binding": {
        "config": {
          "struct": {
            "Path": "{view.custom.CurrentModelItem.Path}",
            "modelItems": "{session.custom.ModelItems}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\treturn value[\"Path\"]\n\tmodelItems \u003d value[\"modelItems\"]\n\tmodelItemTagPath \u003d value[\"Path\"]\n\treturn Standard.Navigation.Common.SearchModelItem(modelItems,modelItemTagPath)[\"data\"][\"Path\"]",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.items": {
      "binding": {
        "config": {
          "path": "session.custom.ModelItems"
        },
        "transforms": [
          {
            "code": "\tmodelItems \u003d value\t\n\ttreeItems \u003d Standard.Navigation.TreeView.GetTreeItems(ModelItems \u003d modelItems)\n\t#Make sure the highest level folder is expanded by default\t\t\n\ttreeItems[0][\"expanded\"] \u003d True\n\treturn treeItems",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.ModelItemTagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "custom": {
              "key": "value"
            },
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tif self.props.value is not None:\n\t\t#Update ModelItemTagPaths Object\n\t\tuseSessionModelItemTagPath \u003d self.session.custom.UseSessionModelItemTagPath\n\t\tcurrentModelItemTagPaths \u003d self.session.custom.CurrentModelItemTagPaths\n\t\tmodelItemTagPath \u003d self.props.value\n\t\tperspectivePageID \u003d self.page.props.pageId\n\t\tself.session.custom.CurrentModelItemTagPaths \u003d Standard.Navigation.Common.UpdateCurrentModelItemTagPaths(UpdatedModelItemTagPath \u003d modelItemTagPath,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t CurrentModelItemTagPaths \u003d currentModelItemTagPaths,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t UseSessionModelItemTagPath \u003d useSessionModelItemTagPath,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t PerspectivePageID \u003d perspectivePageID)\n\t\tself.props.value \u003d None"
                  },
                  "scope": "G",
                  "type": "script"
                }
              },
              "dom": {
                "onKeyDown": {
                  "config": {
                    "script": "\tkey \u003d event[\u0027key\u0027]\n\tif key \u003d\u003d \"Enter\":\n\t\t#system.perspective.print(self.props.search.searchParam)\n\t\tsearchParam \u003d self.props.search.searchParam\n\t\tif len(searchParam)\u003e0:\n\t\t\tself.getSibling(\"IconSearch\").custom.searchString\u003dsearchParam"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Dropdown"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "props.options": {
                "binding": {
                  "config": {
                    "path": "view.custom.items"
                  },
                  "transforms": [
                    {
                      "code": "\toptions\u003d []\n\tdef getOptions(path,items):\n\t\ti\u003d0\n\t\tfor item in items:\n\t\t\toption \u003d {}\n\t\t\toptionPath \u003d path + item[\"data\"][\"TagFolderName\"] + \"/\"\n\t\t\toption[\"label\"] \u003d optionPath[0:-1]\n\t\t\toptionValue \u003d item[\"data\"][\"ModelItemTagPath\"]\n\t\t\toption[\"value\"] \u003d optionValue\n\t\t\toptions.append(option)\n\t\t\tgetOptions(path\u003doptionPath,items\u003ditem[\"items\"])\n\t\t\ti \u003d+1\n\t\treturn \n\t\n\tgetOptions(path\u003d\u0027\u0027,items\u003dvalue)\n\t\n\treturn options",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "dropdownOptionStyle": {
                "borderStyle": "none",
                "classes": "Standard/Docked/Left/Background Standard/Docked/Left/Text"
              },
              "placeholder": {
                "color": "var(--dockedNavigationContentText)",
                "text": "Search for an Area..."
              },
              "style": {
                "borderStyle": "none",
                "classes": "Standard/Docked/Left/Background Standard/Docked/Left/Text"
              },
              "value": null
            },
            "type": "ia.input.dropdown"
          },
          {
            "custom": {
              "searchString": "100M005"
            },
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tsearchParam \u003d self.getSibling(\"Dropdown\").props.search.searchParam\n\tself.custom.searchString \u003d searchParam"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "IconSearch",
              "tooltip": {
                "enabled": true
              }
            },
            "position": {
              "basis": "30px"
            },
            "propConfig": {
              "custom.results": {
                "binding": {
                  "config": {
                    "struct": {
                      "searchString": "{this.custom.searchString}",
                      "tagProviders": "{this.custom.tagProviders}"
                    },
                    "waitOnAll": true
                  },
                  "transforms": [
                    {
                      "code": "\tsearchString \u003d value[\u0027searchString\u0027]\n\ttagProviders \u003d value[\u0027tagProviders\u0027]\n\tif len(searchString)\u003d\u003d0: \n\t\t\treturn \u0027\u0027\n\ttagProviderModules \u003d {}\n\tallModules \u003d []\n\tfor tagProvider in tagProviders:\n\t\t#return tagProvider\n\t\tlimit \u003d 10000\n\t\tmodules \u003d Standard.Navigation.Common.SearchModule(Provider\u003dtagProvider,SearchString\u003dsearchString,Limit\u003dlimit)\n\t\tallModules.extend(modules)\n\t\n\tmodulesLen \u003d len(allModules)\n\tif modulesLen\u003e0:\n\t\tif modulesLen\u003d\u003d1:\n\t\t\tmodelItemTagPath \u003d allModules[0][\u0027modelItemTagPath\u0027]\n\t\t\tinstanceName \u003d allModules[0][\u0027instance\u0027][\u0027name\u0027]\n\t\t\t\n\t\t\t#Write to instance SelfIdentifyRequest\n\t\t\tfullPath \u003d str(allModules[0][\u0027instance\u0027][\u0027fullPath\u0027])\n\t\t\tidentifyTagPath \u003d fullPath + \"/Id/SelfIdentifyRequest\"\n\t\t\tif system.tag.exists(identifyTagPath):\n\t\t\t\tsystem.tag.writeBlocking(tagPaths\u003d[identifyTagPath], values\u003d[True])\n\t\t\t\n\t\t\tuseSessionModelItemTagPath \u003d self.session.custom.UseSessionModelItemTagPath\n\t\t\tcurrentModelItemTagPaths \u003d self.session.custom.CurrentModelItemTagPaths\n\t\t\tperspectivePageID \u003d self.page.props.pageId\n\t\t\tself.session.custom.CurrentModelItemTagPaths \u003d Standard.Navigation.Common.UpdateCurrentModelItemTagPaths(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tUpdatedModelItemTagPath \u003d modelItemTagPath,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t \tCurrentModelItemTagPaths \u003d currentModelItemTagPaths,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t \tUseSessionModelItemTagPath \u003d useSessionModelItemTagPath,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t \tPerspectivePageID \u003d perspectivePageID,SearchModule\u003dinstanceName)\n\t\telse:\n\t\t\t#Show Result Popup\n\t\t\tpopup \u003d \u0027ModulesFoundPopup\u0027\n\t\t\tparams\u003d{\u0027Modules\u0027:allModules}\n\t\t\tsystem.perspective.openPopup(\n\t\t\tid\u003dpopup,\n\t\t\ttitle\u003d\"Results for: \" + searchString,\n\t\t\tview\u003d\u0027Standard/Navigation/Components/ModulesFoundPopup\u0027,\n\t\t\tparams\u003dparams,\n\t\t\tshowCloseButton\u003dTrue,\n\t\t\tviewportBound\u003dTrue,\n\t\t\tmodal\u003dTrue,\n\t\t\toverlayDismiss\u003dTrue\n\t\t\t)\n\t\t\t\n\t\t\n\t\t\t\t\t\t\t\t\n\treturn allModules\n\t",
                      "type": "script"
                    }
                  ],
