{
  "custom": {},
  "params": {},
  "props": {
    "defaultSize": {
      "height": 75,
      "width": 130
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "User Icon"
            },
            "position": {
              "basis": "25px",
              "grow": 1
            },
            "props": {
              "path": "material/person"
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Username"
            },
            "position": {
              "basis": "25px",
              "display": false
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "expression": "!{session.props.auth.authenticated}"
                  },
                  "type": "expr"
                },
                "persistent": true
              }
            },
            "props": {
              "text": "Sign In"
            },
            "type": "ia.display.label"
          }
        ],
        "events": {
          "dom": {
            "onClick": {
              "config": {},
              "scope": "C",
              "type": "login"
            }
          }
        },
        "meta": {
          "name": "User Icon"
        },
        "position": {
          "basis": "50px"
        },
        "props": {
          "direction": "column"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Time"
            },
            "position": {
              "basis": "32px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "dateFormat(now(60000),\"YYYY/MM/dd HH:mm:ss\")"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "textStyle": {
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Username"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "session.props.auth.user.userName"
                  },
                  "type": "property"
                }
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Role"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "session.props.auth.user.roles[0]"
                  },
                  "type": "property"
                }
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "User Details"
        },
        "position": {
          "basis": "100px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if({page.props.dimensions.screen.width}\u003c480,false,{session.props.auth.authenticated})"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "direction": "column"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "justify": "flex-end",
      "style": {
        "classes": "Standard/Docked/Header/ContentBackground Standard/Docked/Header/ContentBorder Standard/Docked/Header/ContentText",
        "fontSize": "10px"
      }
    },
    "type": "ia.container.flex"
  }
}