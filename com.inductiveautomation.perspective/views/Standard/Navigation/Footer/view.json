{
  "custom": {
    "CurrentModelItemTagPath": "[ImpalaDevelopment]Utilities/Cooling Water/Model Item",
    "ViewPathArray": [
      {
        "ContentType": [
          "Overview"
        ],
        "ContentTypeParams": {},
        "Label": "Overview",
        "Order": 0,
        "PagePath": "",
        "Roles": [],
        "ViewPath": "*Overview/Overview",
        "key": "k0"
      },
      {
        "ContentType": [
          "Alarms",
          "Footer Alarms"
        ],
        "ContentTypeParams": {},
        "Label": "Alarms",
        "Order": 2,
        "PagePath": "",
        "Roles": [],
        "ViewPath": "Application/Content Types/Alarms/Alarms Status",
        "key": "k1"
      },
      {
        "ContentType": [
          "Trend"
        ],
        "ContentTypeParams": {},
        "Label": "Trend",
        "Order": 3,
        "PagePath": "",
        "Roles": [],
        "ViewPath": "Application/Content Types/Trends/Trends Chart",
        "key": "k2"
      },
      {
        "ContentType": [
          "Footer CM Legend"
        ],
        "ContentTypeParams": {},
        "Label": "Control Module Legend",
        "Order": "0",
        "PagePath": "",
        "Roles": [],
        "ViewPath": "Application/Navigation/Footer Components/Styling Legend",
        "key": "k3"
      }
    ],
    "contentTypes": [
      {
        "contentType": "Footer CM Legend",
        "label": "Control Module Legend",
        "order": 0
      },
      {
        "contentType": "Footer Alarms",
        "label": "Alarms",
        "order": 2
      }
    ],
    "currentModelItem": {
      "ContentType": "Overview",
      "Path": "0",
      "SearchModule": ""
    }
  },
  "params": {
    "AllowedContentTypes": [
      "Footer Alarms",
      "Footer Trend",
      "Footer Setpoints",
      "Footer CM Legend"
    ]
  },
  "propConfig": {
    "custom.CurrentModelItemTagPath": {
      "binding": {
        "config": {
          "struct": {
            "modelItems": "{session.custom.ModelItems}",
            "path": "{view.custom.currentModelItem.Path}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tmodelItems \u003d value[\"modelItems\"]\n\tpath \u003d value[\"path\"]\n\t\n\treturn Standard.Navigation.Common.GetModelItemByPath(ModelItems \u003d modelItems,Path \u003d path)[\"data\"][\"ModelItemTagPath\"]",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.ViewPathArray": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "CurrentModelItemTagPath": "{view.custom.CurrentModelItemTagPath}"
          },
          "tagPath": "{CurrentModelItemTagPath}/ViewPaths[\u0027ViewPathArray\u0027]"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.contentTypes": {
      "binding": {
        "config": {
          "struct": {
            "allowedContentTypes": "{view.params.AllowedContentTypes}",
            "viewPaths": "{view.custom.ViewPathArray}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tallowedContentTypes \u003d value[\u0027allowedContentTypes\u0027]\n\tviewPaths \u003d value[\u0027viewPaths\u0027]\n\t#contentTypes \u003d Standard.Navigation.Common.GetContentTypes(ModelItemTagPath\u003dcurrentModelItemTagPath,GetOnlyConfiguredContentTypes\u003dTrue,AllowedContentTypes\u003dallowedContentTypes)\n\tcontentTypes \u003d Standard.Navigation.Common.GetContentTypes(ViewPaths\u003dviewPaths,GetOnlyConfiguredContentTypes\u003dTrue,AllowedContentTypes\u003dallowedContentTypes)\n\treturn contentTypes",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.currentModelItem": {
      "binding": {
        "config": {
          "struct": {
            "currentModelItem": "{session.custom.CurrentModelItem}",
            "pageId": "{page.props.pageId}",
            "useSessionModelItem": "{session.custom.UseSessionModelItem}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tcurrentModelItem \u003d value[\u0027currentModelItem\u0027]\n\tuseSessionModelItem \u003d value[\u0027useSessionModelItem\u0027]\n\tpageId \u003d value[\u0027pageId\u0027]\n\treturn Standard.Navigation.Common.GetCurrentModelItem(CurrentModelItem\u003dcurrentModelItem,UseSession\u003duseSessionModelItem,PerspectivePageID\u003dpageId)",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "params.AllowedContentTypes": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 1000,
      "width": 1920
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Previous Button",
              "visible": false
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "path": "Standard/Navigation/Footer Components/Previous Button"
            },
            "type": "ia.display.view"
          },
          {
            "meta": {
              "name": "Placeholder"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Toolbar"
            },
            "position": {
              "basis": "150px"
            },
            "props": {
              "path": "Standard/Navigation/Footer Components/Toolbar"
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Content Container"
            },
            "propConfig": {
              "position.tabIndex": {
                "binding": {
                  "config": {
                    "path": "parent.props.currentTabIndex"
                  },
                  "type": "property"
                }
              },
              "props.params.ContentType": {
                "binding": {
                  "config": {
                    "expression": "{view.custom.contentTypes}[{parent.props.currentTabIndex}][\u0027contentType\u0027]"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "params": {
                "Exists": true
              },
              "path": "Standard/Navigation/Components/Content Container"
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "TabContainer"
        },
        "position": {
          "basis": "300px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "len({this.props.tabs})\u003e0"
              },
              "type": "expr"
            }
          },
          "props.tabs": {
            "binding": {
              "config": {
                "path": "view.custom.contentTypes"
              },
              "transforms": [
                {
                  "code": "\ttabs \u003d []\n\tfor contentType in value:\n\t\t\ttab \u003d contentType[\u0027label\u0027]\n\t\t\ttabs.append(tab)\n\treturn tabs",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "contentStyle": {
            "classes": "Standard/Docked/Footer/ContentBackground Standard/Docked/Footer/ContentBorder Standard/Docked/Footer/ContentText"
          },
          "menuType": "modern",
          "tabStyle": {
            "active": {
              "classes": "Standard/Docked/Footer/ContentBackground Standard/Docked/Footer/ContentBorder Standard/Docked/Footer/ContentText",
              "fontWeight": "bolder"
            },
            "inactive": {
              "classes": "Standard/Docked/Footer/ContentBackground Standard/Docked/Footer/ContentBorder Standard/Docked/Footer/ContentText",
              "fontWeight": "lighter"
            }
          }
        },
        "type": "ia.container.tab"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignContent": "flex-end",
      "direction": "column",
      "style": {
        "classes": "Standard/Docked/Footer/Background"
      }
    },
    "type": "ia.container.flex"
  }
}