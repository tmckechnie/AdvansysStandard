        "transforms": [
          {
            "code": "\treturn type(value)\n\tmy_bytes_value \u003d value#b\u0027[{\"Date\": \"2\"}]\u0027\n\tmy_jsonStr \u003d my_bytes_value.decode(\u0027utf8\u0027).replace(\"\u0027\", \u0027\"\u0027)\n\tmy_json \u003d system.util.jsonDecode(my_jsonStr)\n\t\n\treturn my_json",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
